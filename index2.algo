distinctElements = []
sum = 0

set1 = [3, 1, 7, 9]
set2 = [2, 4, 1, 9, 3]

for element in set1:
    if element not in set2 and element not in distinctElements:
        sum = sum + element
        distinctElements.append(element)

for element in set2:
    if element not in set1 and element not in distinctElements:
        sum = sum + element
        distinctElements.append(element)

Print("Sum of distinct elements:", sum)
